{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  
  "WeatherProviders": {
    "OpenMeteo": {
      "Name": "Open-Meteo",
      "ForecastBaseUrl": "https://api.open-meteo.com/",
      "GeocodingBaseUrl": "https://geocoding-api.open-meteo.com/",
      "TimeoutSeconds": 5,
      "CacheTtlSeconds": 120
    }
  },
  
  "WeatherCache": {
    "TtlMinutes": 60
  },
  
  "Localization": {
    "DefaultLanguage": "en"
  }
}
